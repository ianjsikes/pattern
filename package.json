{
  "name": "pattern",
  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "build": "bankai build && build",
    "dev": "bankai start index.js",
    "inspect": "bankai inspect index.js",
    "pack": "bankai build && build --dir",
    "start": "NODE_ENV=development electron main.js",
    "test": "standard && test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons"
  },
  "build": {
    "appId": "pattern",
    "files": [
      "**/*"
    ],
    "win": {
      "target": [
        "squirrel"
      ]
    }
  },
  "dependencies": {
    "choo": "^6.4.2",
    "choo-devtools": "^1.3.1",
    "choo-log": "^7.2.1",
    "electron-collection": "^1.2.0",
    "tachyons": "^4.8.1"
  },
  "devDependencies": {
    "bankai": "^9.0.0-1",
    "electron": "^1.7.9",
    "electron-builder": "^19.37.2",
    "electron-builder-squirrel-windows": "^19.37.0",
    "standard": "^10.0.2"
  }
}
